{
    "app-id": "org.gnome.font-viewer",
    "runtime": "org.gnome.Platform",
    "runtime-version": "3.32",
    "sdk": "org.gnome.Sdk",
    "command": "gnome-font-viewer",
    "finish-args": [
        "--device=dri",
        "--share=ipc",
        "--socket=wayland",
        "--socket=x11",
        "--socket=fallback-x11",
        "--talk-name=org.gtk.vfs", "--talk-name=org.gtk.vfs.*"
    ],
    "cleanup": [
        "/include",
        "/lib/pkgconfig", "/share/pkgconfig",
        "/share/aclocal",
        "/man", "/share/man", "/share/gtk-doc",
        "*.la", ".a",
        "/lib/girepository-1.0",
        "/share/gir-1.0",
        "/share/doc"
    ],
    "modules": [
        {
            "name": "gnome-desktop",
            "buildsystem": "meson",
            "builddir": true,
            "config-opts": [
                "--libdir=/app/lib"
            ],
            "sources": [
                {
                    "type": "git",
                    "url": "https://gitlab.gnome.org/GNOME/gnome-desktop.git",
                    "tag": "3.32.0",
                    "commit": "0e76fa7937cde35189c74f93ef6d2081d52bf9b5"
                }
            ]
        },
        {
            "name": "gnome-font-viewer",
            "buildsystem": "meson",
            "builddir": true,
            "sources": [
                {
                    "type": "git",
                    "url": "https://gitlab.gnome.org/GNOME/gnome-font-viewer.git",
                    "tag": "3.32.0",
                    "commit": "50fa8504ed1bffb75ef9957c67a6cf26d8efaf34"
                }
            ]
        }
    ]
}
